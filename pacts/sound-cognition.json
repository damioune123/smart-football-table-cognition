{
  "consumer": {
    "name": "sound"
  },
  "provider": {
    "name": "cognition"
  },
  "messages": [
    {
      "description": "id of team that scored",
      "metaData": {
        "contentType": "application/json; charset\u003dUTF-8"
      },
      "contents": {
        "payload": "1",
        "topic": "team/scored"
      },
      "matchingRules": {
        "body": {
          "$.topic": {
            "matchers": [
              {
                "match": "type"
              }
            ],
            "combine": "AND"
          },
          "$.payload": {
            "matchers": [
              {
                "match": "regex",
                "regex": "\\d*"
              }
            ],
            "combine": "AND"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.0"
    }
  }
}
